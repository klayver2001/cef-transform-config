<% // backend/src/templates/cisco-com_switch-tipo1.ejs %>
!
! OWNER OI/SENCINET - CISCO 1905/1921 COM SWITCH
! VRRP_<%= codigoUl %>_LinkOI
!
service timestamps debug datetime localtime
service timestamps log datetime localtime
service password-encryption
!
clock timezone BRA -3

hostname <%= codigoUl %>_RT02
!
boot-start-marker
boot-end-marker
!
username sysop privilege 15 password 0 PDOc8N93!XNtREFx$
enable password PDOc8N93!XNtREFx$
!
aaa new-model
!
!
aaa group server tacacs+ fac-ctc
server name fac-ctc
ip tacacs source-interface Loopback10
!
aaa group server tacacs+ fac-dtc
server name fac-dtc
ip tacacs source-interface Loopback10
!
aaa authentication login default group fac-ctc group fac-dtc local
aaa authentication ppp default local
aaa authorization config-commands
aaa authorization exec default group fac-ctc group fac-dtc local
!
!
aaa session-id common
!
memory-size iomem 25
!
no ipv6 cef
ip source-route
ip cef
!
!
ip multicast-routing
!
!
no ip domain lookup
ip domain name sencinet.com
!
multilink bundle-name authenticated
!
crypto pki token default removal timeout 0
!
redundancy
!
controller Cellular 0/0
!
ip tftp source-interface Loopback10
ip ssh version 2
!
interface Loopback10
description gerenciamento CEF circuito BRISANET (secundario)
ip address <%= ipLoopback10 %> 255.255.255.255
no shutdown
!
interface Embedded-Service-Engine0/0
no ip address
shutdown
!
interface GigabitEthernet0/0
description REDE LAN UL - Terminais
ip address <%= ipRedeVlan3 %> 255.255.255.240
ip address <%= ipSwitchRouter %> 255.255.255.248 secondary
vrrp 1 ip <%= ipRedeVlan1 %>
vrrp 1 description REDE LAN UL
vrrp 2 ip <%= ipSwitchVip %>
vrrp 2 description REDE SWITCH
ip helper-address 172.31.248.215
ip helper-address 172.31.252.215
duplex auto
speed auto
no shut
!
interface GigabitEthernet0/1
description Conectado ao circuito BRISANET
ip address <%= ipWanBkp2 %> 255.255.255.252
ip pim sparse-mode
ip igmp join-group 230.1.1.2
ip igmp static-group 230.1.1.2
duplex auto
speed auto
no keepalive
no shutdown
!
interface Cellular0/0/0
shutdown
!
interface Cellular0/0/1
shutdown
!
ip prefix-list ADV_BRISANET seq 10 permit 99.244.0.0/14 le 28
ip prefix-list ADV_BRISANET seq 20 permit 10.50.128.0/17 le 32
!
ip prefix-list LAN_TFL seq 10 permit 99.244.0.0/14 le 28
ip prefix-list LAN_TFL seq 20 deny 0.0.0.0/0 le 32
!
ip prefix-list ROTAS_DC seq 10 permit 172.16.0.0/16 le 32
ip prefix-list ROTAS_DC seq 20 permit 172.17.0.0/16 le 32
ip prefix-list ROTAS_DC seq 30 permit 172.18.0.0/16 le 32
ip prefix-list ROTAS_DC seq 40 permit 172.24.0.0/16 le 32
ip prefix-list ROTAS_DC seq 50 permit 172.27.0.0/16 le 32
ip prefix-list ROTAS_DC seq 60 permit 172.31.0.0/16 le 32
ip prefix-list ROTAS_DC seq 70 permit 200.218.181.0/24 le 32
!
route-map ROTAS_DC permit 10
match ip address prefix-list ROTAS_DC
!
route-map ADV_BRISANET permit 10
match ip address prefix-list ADV_BRISANET
!
router bgp 64765
bgp router-id <%= ipLoopback10 %>
bgp log-neighbor-changes
no bgp default ipv4-unicast
timers bgp 10 30
neighbor <%= ipWanBkp1 %> remote-as 64999
neighbor <%= ipWanBkp1 %> description BRISANET
!
address-family ipv4
neighbor <%= ipWanBkp1 %> activate
neighbor <%= ipWanBkp1 %> soft-reconfiguration inbound
neighbor <%= ipWanBkp1 %> route-map ROTAS_DC in
neighbor <%= ipWanBkp1 %> route-map ADV_BRISANET out
exit-address-family
!
ip forward-protocol nd
!
ip pim rp-address 172.17.150.33
no ip http server
no ip http secure-server
!
ip route 0.0.0.0 0.0.0.0 <%= ipWanBkp1 %>
!
!
ip sla 1
icmp-echo 10.98.0.1 source-interface Loopback10
frequency 300
ip sla schedule 1 life forever start-time now
!
logging trap notifications
logging origin-id hostname
logging source-interface Loopback10
logging host 172.31.249.36 transport udp port 5518
logging host 172.31.253.36 transport udp port 5518
access-list 1 permit any
access-list 50 permit 172.31.249.0 0.0.0.255
access-list 50 permit 172.31.253.0 0.0.0.255
access-list 50 permit 200.218.181.0 0.0.0.255
access-list 50 deny   any log
access-list 50 permit 172.31.248.0 0.0.3.255
access-list 50 permit 172.31.252.0 0.0.3.255
dialer-list 1 protocol ip permit
!
snmp-server group GRP_LOTERICOS v2c write lotericos
snmp-server group GRP_LOTERICOS v3 auth write lotericos
snmp-server group GRP_LOTERICOS v3 priv write lotericos
snmp-server view lotericos iso included
snmp-server community lotericos RW 50
snmp-server trap-source Loopback10
snmp-server source-interface informs Loopback10
snmp-server manager
tacacs server fac-ctc
address ipv4 172.31.248.45
key Consorcio123@
tacacs server fac-dtc
address ipv4 172.31.252.45
key Consorcio123@
!
control-plane
!
